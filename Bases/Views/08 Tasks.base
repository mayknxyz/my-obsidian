views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder != "Templates"
        - type == ["Task"]
    order:
      - file.name
      - status
      - priority
      - due
      - scheduled
      - context
      - dept
      - estimate
      - pomodoros
      - recurrence
      - blocked
      - completed
      - parent
      - project
    columnSize: {}
