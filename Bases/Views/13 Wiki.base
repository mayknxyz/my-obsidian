views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder != "Templates"
        - type == ["Wiki"]
    order:
      - file.name
      - status
      - type
      - parent
      - project
      - account
      - area
      - priority
      - due
      - created
      - link
      - wiki
      - tags
    columnSize:
      note.wiki: 129
